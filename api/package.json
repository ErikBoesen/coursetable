{
  "scripts": {
    "start": "ts-node-dev src/server.js",
    "prod": "NODE_ENV=production ts-node src/server.js",
    "checks": "yarn run depcheck && yarn run lint && yarn run typecheck",
    "depcheck": "npx depcheck . --json",
    "lint": "prettier -l src && (eslint src || true)",
    "typecheck": "tsc --noEmit && (tsc --noEmit --checkJs || true)"
  },
  "nodemonConfig": {
    "ignore": [
      "static/*"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "graphqurl": "^0.3.3",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.16",
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1"
  }
}
